	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"containers": [
		{
			"max_instances": 10,
			"class_name": "MyContainer",
			"image": "./Dockerfile",
			"instance_type": "standard-2"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "MY_CONTAINER",
				"class_name": "MyContainer"
			}
		]
	},
	"migrations": [
		{
			"tag": "v1",
			"new_sqlite_classes": ["MyContainer"]
		}
	],
	"services": [
		{
			"binding": "BROWSER_WORKER",
			"service": "browser-worker"
		}
	]
}
